 {{#each articles}} {{#unless isSaved}}
<div class="row">
  <div class="col s12">

    <div class="card horizontal" data-id={{_id}}>
      <div class="card-stacked">
        <div class="card-content">
          <a href="/article/{{_id}}"><h2>{{title}}</h2></a>
          <div id="noteContent">
            <div class="noteTitle">

            </div>
            <div class="noteText">

            </div>
          </div>
        </div>
        <div class="card-action">
          <a href={{link}}>{{link}}</a>

          <button class="btn buttonFloatRight save" data-id={{_id}}>Save</button>
        </div>
      </div>
    </div>
  </div>
</div>
{{/unless}} {{else}}
<p>No Articles to grab</p>

{{/each}}

<script
  src="https://code.jquery.com/jquery-3.3.1.slim.min.js"
  integrity="sha256-3edrmyuQ0w65f8gfBsqowzjJe2iM6n0nKciPUp8y+7E="
  crossorigin="anonymous"></script>