 {{#each articles}} {{#unless isSaved}}
<div class="row">
  <div class="col s12">

    <div class="card horizontal" data-id=>
      <div class="card-stacked">
        <div class="card-content">
          <a href="/article/"><h2>{{title}}</h2></a>
          <div id="noteContent">
            <div class="noteTitle">

            </div>
            <div class="noteText">

            </div>
          </div>
        </div>
        <div class="card-action">
          <a href={{link}}>{{link}}</a>

          <button class="btn buttonFloatRight save" data-id=>Save</button>
        </div>
      </div>
    </div>
  </div>
</div>
{{/unless}} {{else}}
<p>No Articles to grab</p>

{{/each}}